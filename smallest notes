#include <iostream>
#include<stack>
using namespace std;

int main()
{
    ios_base::sync_with_stdio(false);
	cin.tie(NULL);

    int i;
    cin>>i;
    while(i--)
    {   int j;
        cin>>j;
        int k=0;
        k=k+j/100;
        j=j%100;
         k=k+j/50;
        j=j%50;
         k=k+j/10;
        j=j%10;
         k=k+j/5;
        j=j%5;
         k=k+j/2;
        j=j%2;
         k=k+j/1;
        j=j%1;

        cout<<k<<endl;
    }
}
